                @Html.DisplayFor(modelItem => item.IdCriacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataModificacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdModificacao)
            </td>
