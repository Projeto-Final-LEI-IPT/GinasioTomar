            <td>
                @Html.DisplayFor(modelItem => item.DescricaoRubrica)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstadoRubrica)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoMovimento)
