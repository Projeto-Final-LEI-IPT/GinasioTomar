                @Html.DisplayFor(modelItem => item.LocalTreino)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Horario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HorasSemanais)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataCriacao)
            </td>
