            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rubrica[0].OrdemPrecario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rubrica[0].IVlrUnit)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rubrica[0].ValorUnitario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rubrica[0].LocalTreino)
