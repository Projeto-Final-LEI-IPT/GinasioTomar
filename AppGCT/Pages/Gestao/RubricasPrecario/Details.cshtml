@page
@model AppGCT.Pages.Gestao.RubricasPrecario.DetailsModel

@{
    ViewData["Title"] = "Detalhes Rúbrica Preçário";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>Detalhes Rúbrica Preçário</h1>

<hr />
<div>
    <a class='btn btn-primary btn-sm' asp-area='' asp-page='/Gestao/RubricasPrecario/Index'>Voltar</a>
</div>
<br>
<div>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Rubrica.CodRubrica)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Rubrica.CodRubrica)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Rubrica.DescricaoRubrica)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Rubrica.DescricaoRubrica)
        </dd>
        <dt class="col-sm-2">
            Classe
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Rubrica.Modalidade.NomeClasse)
        </dd>
        <dt class="col-sm-2">
            Desconto
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Rubrica.Discount.DescDesconto)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Rubrica.EstadoRubrica)
        </dt>
        <dd class="col-sm-10">
            @Html.RadioButtonFor(model => model.Rubrica.EstadoRubrica, "A", new { @disabled = "disabled" }) Ativo
            @Html.RadioButtonFor(model => model.Rubrica.EstadoRubrica, "I", new { @disabled = "disabled" }) Inativo
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Rubrica.TipoMovimento)
        </dt>
        <dd class="col-sm-10">
            @Html.RadioButtonFor(model => model.Rubrica.TipoMovimento, "D", new { @disabled = "disabled" }) Débito
            @Html.RadioButtonFor(model => model.Rubrica.TipoMovimento, "C", new { @disabled = "disabled" }) Crédito
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Rubrica.IPrecario)
        </dt>
        <dd class="col-sm-10">
            @Html.RadioButtonFor(model => model.Rubrica.IPrecario, "S", new { @disabled = "disabled" }) Sim
            @Html.RadioButtonFor(model => model.Rubrica.IPrecario, "N", new { @disabled = "disabled" }) Não
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Rubrica.OrdemPrecario)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Rubrica.OrdemPrecario)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Rubrica.IVlrUnit)
        </dt>
        <dd class="col-sm-10">
             @Html.RadioButtonFor(model => model.Rubrica.IVlrUnit, "S", new { @disabled = "disabled" }) Sim
            @Html.RadioButtonFor(model => model.Rubrica.IVlrUnit, "N", new { @disabled = "disabled" }) Não
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Rubrica.ValorUnitario)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Rubrica.ValorUnitario)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Rubrica.LocalTreino)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Rubrica.LocalTreino)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Rubrica.Horario)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Rubrica.Horario)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Rubrica.HorasSemanais)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Rubrica.HorasSemanais)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Rubrica.DataCriacao)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Rubrica.DataCriacao)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Rubrica.IdCriacao)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Rubrica.IdCriacao)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Rubrica.DataModificacao)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Rubrica.DataModificacao)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Rubrica.IdModificacao)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Rubrica.IdModificacao)
        </dd>
    </dl>
</div>
<div>
    <a class='btn btn-primary btn-sm' asp-area='' asp-page='/Gestao/RubricasPrecario/Edit' asp-route-id='@Model.Rubrica?.CodRubrica'>Editar</a>
</div>