@page
@model AppGCT.Pages.Inscricoes.InscricaoGinasta.IndexModel

@{
    ViewData["Title"] = "Index";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>Index</h1>
<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Ginasta[0].NomeCompleto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ginasta[0].DtNascim)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ginasta[0].EstadoGinasta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ginasta[0].Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ginasta[0].Socio)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Ginasta) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomeCompleto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DtNascim)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstadoGinasta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Socio.NumSocio)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a> |
                <a asp-page="/Inscricoes/InscricaoEpoca/Index" asp-route-id="@item.Id">Inscrições</a>
            </td>
        </tr>
}
    </tbody>
</table>
