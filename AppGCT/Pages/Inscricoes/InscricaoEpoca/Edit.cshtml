@page
@model AppGCT.Pages.Inscricoes.InscricaoEpoca.EditModel

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>Edit</h1>

<h4>Inscricao</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Inscricao.Id" />
            <div class="form-group">
                <label asp-for="Inscricao.IdFGP" class="control-label"></label>
                <input asp-for="Inscricao.IdFGP" class="form-control" />
                <span asp-validation-for="Inscricao.IdFGP" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Inscricao.ISocGinasta" class="control-label"></label>
                <input asp-for="Inscricao.ISocGinasta" class="form-control" />
                <span asp-validation-for="Inscricao.ISocGinasta" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Inscricao.GinastaId" class="control-label"></label>
                <select asp-for="Inscricao.GinastaId" class="form-control" asp-items="ViewBag.GinastaId"></select>
                <span asp-validation-for="Inscricao.GinastaId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="./Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
